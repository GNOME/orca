#: ../src/orca/scripts/StarOffice.py:692
#: ../src/orca/scripts/StarOffice.py:2640
#: ../src/orca/scripts/StarOffice.py:2658 ../src/orca/where_am_I.py:1457
msgid "blank"
msgstr "prázdný"

#. Translators: this is the number of space characters on a line
#. of text.
#.
#: ../src/orca/default.py:2164
#, python-format
msgid "%d space"
msgid_plural "%d spaces"
msgstr[0] "%d mezera"
msgstr[1] "%d mezery"
msgstr[2] "%d mezer"

#. Translators: this is the number of tab characters on a line
#. of text.
#.
#: ../src/orca/default.py:2171
#, python-format
msgid "%d tab"
msgid_plural "%d tabs"
msgstr[0] "%d tabulátor"
msgstr[1] "%d tabulátory"
msgstr[2] "%d tabulátorů"

#. Translators: this is an index value
#. so that we can tell which progress bar
#. we are referring to.
#.
#: ../src/orca/default.py:2496
#, python-format
msgid "Progress bar %d."
msgstr "Ukazatel průběhu ukazuje %d"

#. Translators: this is the percentage value of a
#. progress bar.
#.
#. Translators: this is the percentage value of a slider,
#. progress bar or other component that displays a value as
#. a percentage.
#.
#. Translators: this is the percentage value of a progress bar.
#.
#: ../src/orca/default.py:2504 ../src/orca/flat_review.py:444
#: ../src/orca/speechgenerator.py:1006
#, python-format
msgid "%d percent."
msgstr "%d procent"

#. Translators: this represents the depth of a node in a tree
#. view (i.e., how many ancestors a node has).
#.
#: ../src/orca/default.py:2718 ../src/orca/scripts/gaim.py:298
#: ../src/orca/where_am_I.py:619 ../src/orca/where_am_I.py:680
#, python-format
msgid "tree level %d"
msgstr "Úroveň stromu %d"

#. Translators: this is in reference to a table cell being
#. selected or not.
#.
#. ONLY TRANSLATE THE PART AFTER THE PIPE CHARACTER |
#.
#: ../src/orca/default.py:2744
msgid "tablecell| not selected"
msgstr " nevybrána"

#. Translators: this means the user has selected
#. all the text in a document (e.g., Ctrl+a in gedit).
#.
#: ../src/orca/default.py:3181
msgid "entire document selected"
msgstr "vybraný celý dokument"

#. Translators: this object is now selected.
#. Let the user know this.
#.
#. ONLY TRANSLATE THE PART AFTER THE PIPE
#. CHARACTER |
#.
#. Translators: when the user selects (highlights) text in
#. a document, Orca lets them know this.
#.
#. ONLY TRANSLATE THE PART AFTER THE PIPE CHARACTER |
#.
#: ../src/orca/default.py:3490 ../src/orca/default.py:3711
#: ../src/orca/default.py:6666 ../src/orca/scripts/StarOffice.py:170
#: ../src/orca/speechgenerator.py:238 ../src/orca/where_am_I.py:493
#: ../src/orca/where_am_I.py:893
msgid "text|selected"
msgstr "vybráno"

#. Translators: this object is now unselected.
#. Let the user know this.
#.
#. ONLY TRANSLATE THE PART AFTER THE PIPE
#. CHARACTER |
#.
#. Translators: when the user unselects
#. (unhighlights) text in a document, Orca lets
#. them know this.
#.
#. ONLY TRANSLATE THE PART AFTER THE PIPE CHARACTER |
#.
#: ../src/orca/default.py:3499 ../src/orca/default.py:6682
msgid "text|unselected"
msgstr "nevybráno"

#. Translators: when users are navigating a table, they
#. sometimes want the entire row of a table read, or
#. they just want the current cell to be presented to them.
#.
#: ../src/orca/default.py:3768
msgid "Speak row"
