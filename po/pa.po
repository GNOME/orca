msgid "%d space"
msgid_plural "%d spaces"
msgstr[0] "%d ਸਪੇਸ "
msgstr[1] "%d ਸਪੇਸ"

#. Translators: this is the number of tab characters on a line
#. of text.
#.
#: ../src/orca/default.py:2233
#, python-format
