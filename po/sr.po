#: src/orca/guilabels.py:1023
#, python-format
msgid "%d notification"
msgid_plural "%d notifications"
msgstr[0] "%d обавештење"
msgstr[1] "%d обавештења"
msgstr[2] "%d обавештења"
msgstr[3] "%d обавештење"

