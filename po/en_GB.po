#, python-format
msgid "%d tab"
msgid_plural "%d tabs"
msgstr[0] "%d tab"
msgstr[1] "%d tabs"

#. Translators: This message presents the number of tables in a document.
#: src/orca/messages.py:2830
#, python-format
msgid "%d table"
msgid_plural "%d tables"
msgstr[0] "%d table"
msgstr[1] "%d tables"

