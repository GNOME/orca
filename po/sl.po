#: ../orca.desktop.in.h:1
msgid "Orca Screen Reader and Magnifier"
msgstr "Povečevalo in bralnik zaslona Orka"

#: ../orca.desktop.in.h:2
msgid "Present on-screen information as speech or braille, or magnify the screen"
msgstr "Vsebino zaslona prikaže v Braillovi pisavi, jo prebere ali pa poveča zaslon"

#: ../src/orca/app_gui_prefs.py:121
#, python-format
msgid "Orca Preferences for %s"
msgstr "Nastavitve Orke za %s"

#: ../src/orca/app_gui_prefs.py:187
