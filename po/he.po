#: src/orca/guilabels.py:967
#, python-format
msgid "%d notification"
msgid_plural "%d notifications"
msgstr[0] "התראה %d"
msgstr[1] "%d התראות"
msgstr[2] "%d התראות"
msgstr[3] "%d התראות"

