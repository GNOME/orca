#: ../src/orca/speechgenerator.py:1400 ../src/orca/where_am_I.py:605
#: ../src/orca/where_am_I.py:666
msgid "collapsed"
msgstr "свито"

#. Translators: this represents the depth of a node in a tree
#. view (i.e., how many ancestors a node has).
#.
#: ../src/orca/braillegenerator.py:686
#, python-format
msgid "LEVEL %d"
msgstr "НИВО %d"

